{
  "name": "gymhit_api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn tsc --build",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main/index.ts",
    "typeorm": "typeorm-ts-node-commonjs -d src/data/typeorm",
    "migration:run": "yarn typeorm migration:run",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:create": "typeorm-ts-node-commonjs migration:create src/data/typeorm/migrations/",
    "migration:show": "yarn typeorm migration:show",
    "migration:revert": "yarn typeorm migration:revert",
    "test": "yarn jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:unit": "yarn test -- --watch -c jest-unit.config.js",
    "test:integration": "yarn test -- --watch -c jest-integration.config.js",
    "test:ci": "yarn test -- --coverage",
    "lint": "yarn eslint src --fix --cache"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3",
    "husky": "^8.0.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2"
  }
}
